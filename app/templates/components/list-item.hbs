{{#each items as |item|}}
  <div class="col-md-12">
    <div class="panel panel-default text-center">
      <span class="border-top-0" {{action "openModal"}}>
        <p onClick=>{{item.title}}</p>
        {{modal-component modalOpen=modalOpen item=item listId=listId}}
      </span>
    </div>
  </div>
{{/each}}
