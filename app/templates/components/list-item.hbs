{{#each items as |item|}}
  <div class="col-md-12">
    <div class="panel panel-default text-center">
      <span class="border-top-0" onClick={{action "openModal"}}>
        <p>{{item.title}}</p>
          {{modal-component modalOpen=modalOpen item=item listId=listId deleteItem=(action deleteItem)}}
      </span>
    </div>
  </div>
{{/each}}
