<h2>{{title}}</h2>

{{#each items as |item|}}
  <div class="col-md-12">
    <div class="panel panel-default text-center">
      <span class="border-top-0">
        <p>{{item}}</p>
      </span>
    </div>
  </div>
{{/each}}

<form {{action "sendDataToController" listId item on="submit"}}>
  {{input value=item placeholder="Add New Item to List"}}
</form>
